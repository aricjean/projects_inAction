<%- include header %>

<form method="post">
	<div class="form-group">
		<input type="text" class="form-control" name="username" placeholder="请输入用户名">
	</div>

	<div class="form-group">
		<input type="password" class="form-control" name="password" placeholder="请输入密码">
	</div>

	<div class="form-group">
		<input type="password" class="form-control" name="passwordRepeat" placeholder="请再次输入密码">
	</div>

	<div class="form-group">
		<input type="email" class="form-control" name="email" placeholder="请输入邮箱">
	</div>
	
	<button type="submit" class="btn btn-primary center-block">LogUp</button>
</form>

<%- include footer%>